@using Proyecto.Models;
@{

    var lista = new List<almacen>();

    if (ViewBag.bodega != null)
    {
        lista = (List<almacen>)ViewBag.bodega;
    }

}
<form class="Producto-form" method="post" action="/Producto/almacen">
    <div class="form-group">

        <label for="username">Lugar de almacen </label>

        <input type="text" class="form-control" name="Nombre" placeholder="Ingresa el nombre bodega" required>
    </div>
    <button type="submit" class="btn btn-primary btn-block">Enviar datos</button>
</form>

<div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in lista)
            {
                <tr>
                    <th scope="row">
                        @item.Id



                    </th>
                    <td>
                        @item.Nombre
                    </td>

                </tr>
            }



        </tbody>
    </table>
</div>
