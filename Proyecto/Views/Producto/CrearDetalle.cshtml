@using Proyecto.Models;
@{

    var lista = new List<Empaque>();

    if (ViewBag.juan != null)
    {
        lista = (List<Empaque>)ViewBag.juan;
    } 

}
<form class="Producto-form" method="post" action="/Producto/GuardarEmpaque">
    <div class="form-group">

        <label for="username">Nombre de Usuario</label>

        <input type="text" class="form-control" name="Nombre" placeholder="Ingresa el nombre de producto" required>
    </div>
    <button type="submit" class="btn btn-primary btn-block">Enviar datos</button>
</form>

<div>
   
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in lista)
            {
                <tr>
                    <th scope="row">
                        @item.Id
                        


                    </th>
                    <td>
                        @item.Nombre
                    </td>

                </tr>
            }
            


        </tbody>
    </table>
</div>
